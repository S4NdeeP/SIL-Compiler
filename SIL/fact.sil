decl
integer fact(integer num);
enddecl

integer fact(integer num){

decl 
integer f;
enddecl

begin
	if(num < 0)
	then 
		f = 0;
	else	

		if(num == 0)
		then
			f = 1;
		else
			f = num * fact(num-1);
		endif;
	endif;
return f;
end
}


